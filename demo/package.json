{
  "name": "storeways",
  "version": "1.0.0",
  "description": "A lightweight, open-source JavaScript plugin to create interactive store locators using OpenStreetMap and Leaflet.js",
  "main": "storeways.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run minify",
    "minify": "terser storeways.js -o ../dist/storeways.min.js -c -m",
    "copy": "copy storeways.js ..\\dist\\storeways.js"
  },
  "keywords": [
    "store-locator",
    "maps",
    "leaflet",
    "openstreetmap",
    "store-finder",
    "location-finder",
    "interactive-map",
    "geolocation",
    "distance-calculator",
    "javascript",
    "vanilla-js"
  ],
  "author": "Dhruvang Gajjar",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dhruvangg/storeways.git"
  },
  "bugs": {
    "url": "https://github.com/dhruvangg/storeways/issues"
  },
  "homepage": "https://github.com/dhruvangg/storeways#readme",
  "peerDependencies": {
    "handlebars": "^4.7.0",
    "leaflet": "^1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "terser": "^5.24.0"
  },
  "files": [
    "storeways.js",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
